<%- if controller_name != 'sessions' %>
  <%= link_to "Log In", new_session_path(resource_name), class: "btn btn-outline-secondary" %>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Register", register_path, class: "btn btn-outline-secondary" %>
<% end %>
